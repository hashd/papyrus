{
  "name": "papyrus",
  "version": "0.0.1",
  "description": "Dynamic Visualization Playground",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run lite\" \"npm run build-app\" ",
    "lite": "lite-server -c config/lite-server.json",
    "build-app": "jspm bundle-sfx app public/app.js --minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "data",
    "dynamic",
    "visualization",
    "chart"
  ],
  "author": "hashd <kd@danduprolu.com>",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^2.0.0",
    "jspm": "^0.16.31",
    "lite-server": "^2.1.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-beta.12",
      "es6-shim": "github:es-shims/es6-shim@^0.35.0",
      "jquery": "npm:jquery@^2.2.2",
      "lodash": "npm:lodash@^4.6.1",
      "zone.js": "npm:zone.js@^0.6.6"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  }
}
